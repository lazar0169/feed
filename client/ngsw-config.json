{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/AppImages/**",
          "/*.png",
          "/*.jpg",
          "/*.svg",
          "/*.ico"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "supabase-api",
      "urls": [
        "https://*.supabase.co/**"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "1h",
        "timeout": "10s",
        "strategy": "freshness"
      }
    },
    {
      "name": "api-cache",
      "urls": [
        "https://*.supabase.co/rest/**"
      ],
      "cacheConfig": {
        "maxSize": 50,
        "maxAge": "30m",
        "strategy": "performance"
      }
    }
  ]
}
